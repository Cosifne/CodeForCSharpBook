        <TextBox x:Name="tbStatus" HorizontalAlignment="Left" Height="55" Margin="34,225,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="448" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
